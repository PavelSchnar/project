import random


def game():

    #main_number = input('Загадай число от 1 до 100')
    number = random.randint(1, 100)
    #print(number)

    user_number = None
    count = 0
    levels = {1: 10, 2: 5, 3: 3}
    level = int(input('Введите уровень сложности 1,2 или 3: '))
    max_count = levels[level]
    user_count = int(input('Укажите количество пользователей: '))
    users = []
    for i in range(user_count):
        user_name = input(f'Введите имя пользователя {i}: ')

        users.append(user_name)
    print(users)
    is_winner = False
    winner_name = None

    while not is_winner:
        count += 1
        if count > max_count:
            print('Все продули!!')
            break
        print(f'Попытка № {count}')

        for user in users:
            print(f'Ход пользователя {user}')
            user_number = int(input('Введите число:'))  # Предложить ввести число
            if user_number == number:
                is_winner = True
                winner_name = user
                break
            elif user_number > number:
                print('Перебор')
            else:
                print('Недобор')
    else:
        print('Угадал')
        print(f'Победитель {winner_name}')


if __name__ == '__main__':
    game()
